{
    "experiment_name": "rem/resnet/no-pruning",
    "tensorboard": false,
    "dataset": "cifar10",
    "num_classes": 10,
    "input_channels": 3,
    "input_height": 32,
    "input_width": 32,
    "shift_pixels": 0,
    "augmentation": true,
    "mean": [0.485, 0.456, 0.406],
    "std": [0.229, 0.224, 0.225],

    "model": "ResNet",
    "start_epoch": 0,
    "epochs": 100,
    "batch_size": 128,
    "optimizer": "adam",
    "lr": 1e-4,
    "min_lr": 0,
    "decay_rate": 0.99,
    "decay_steps": 1,
    "monitor": "loss",
    "iter_size": 1,
    "patience": 200,
    "use_cuda": true,
    "cuda_device": "cuda:6",
    "num_workers": 4,
    "pin_memory": true,
    "print_every": 10,
    "save_every": 1,
    "seeds": [42]
}